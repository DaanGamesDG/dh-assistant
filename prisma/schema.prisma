// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model botBlacklist {
  id String @id
}

model modlog {
  caseId    Int     @id @default(autoincrement())
  id        String // userId-guildId
  moderator String
  reason    String
  type      String
  startDate BigInt
  endDate   BigInt?
}

model Level {
  id    String @id // userId-guildId
  level Int
  xp    Int
  bg    Int
}

model Session {
  id           String @id // session id
  accessToken  String
  refreshToken String
  userId       String
  lastUsed     BigInt
}
