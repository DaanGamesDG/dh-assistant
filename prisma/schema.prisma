// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model botBlacklist {
  id String @id
}

model modlog {
  caseId    String  @id
  userId    String
  guildId   String
  moderator String
  reason    String
  type      String
  startDate BigInt
  endDate   BigInt?
}
